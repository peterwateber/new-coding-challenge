{
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "curly": false,
        "indent": [
            true,
            "tabs"
        ],
        "no-empty": false,
        "no-string-literal": false,
        "object-literal-key-quotes": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            true,
            {
                "import-sources-order": "any",
                "named-imports-order": "case-insensitive"
            }
        ],
        "space-before-function-paren": [
            true,
            {
                "anonymous": "always",
                "asyncArrow": "always",
                "named": "never"
            }
        ]
    },
    "rules": {
        "align": false,
        "arrow-return-shorthand": false,
        "class-name": true,
        "curly": false,
        "forin": false,
        "indent": [
            true,
            "tabs"
        ],
        "interface-name": [
            false
        ],
        "jsx-alignment": false,
        "jsx-no-lambda": false,
        "max-classes-per-file": false,
        "max-line-length": [
            false
        ],
        "member-access": [
            true,
            "check-accessor"
        ],
        "member-ordering": [
            false
        ],
        "no-consecutive-blank-lines": false,
        "no-duplicate-string": false,
        "no-empty": false,
        "no-empty-interface": false,
        "no-identical-functions": false,
        "no-invalid-await": false,
        "no-namespace": [
            false
        ],
        "no-string-literal": false,
        "no-trailing-whitespace": true,
        "no-var-requires": false,
        "object-literal-key-quotes": false,
        "object-literal-sort-keys": false,
        "ordered-imports": [
            true,
            {
                "import-sources-order": "any",
                "named-imports-order": "case-insensitive"
            }
        ],
        "prefer-immediate-return": false,
        "semicolon": [
            true,
            "always",
            "ignore-bound-class-methods"
        ],
        "quotemark": [
            true,
            "double"
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "whitespace": [
            true,
            "check-module"
        ],
        "ban-types": false
    }
}